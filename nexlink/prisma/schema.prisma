generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String   @id @default(cuid())
  clerkId   String   @unique     
  email     String   @unique
  firstName String?              
  lastName  String?             
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("users")
}